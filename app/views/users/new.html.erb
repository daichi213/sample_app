<% provide(:title , "Sign up") %>
<% provide(:button_text , "Create my account") %>
<!-- form_forの送信先の指定（これを指定しないと送信失敗時にurlがresourcesの影響でsignupからusersへと変更される） -->
<% provide(:form_for_destination, signup_path) %>
<h1>Sign up</h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= render partial: 'form', locals: {group: @user} %>
    </div>
</div>